{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"jsx": "preserve",
		"lib": [
			"dom",
			"esnext"
		],
		"moduleResolution": "node",
		"noEmit": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "es5",
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"isolatedModules": true
	},
	"include": [
		"src",
		"react-app-env.d.ts"
	],
	"exclude": [
		"node_modules"
	]
}